+	gnutls_handshake_set_timeout(session, get_timeout() * SRPTIMEOUTMULTIPLIER );
 
 	do {
 		ret = gnutls_handshake(session);
 	} while (ret < 0 && gnutls_error_is_fatal(ret) == 0);
